{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ContactUs from \"./components/ContactUs.vue\"; // 确保这里的路径是正确的\nimport ChatBox from \"./components/ChatBox.vue\"; // 引入你的聊天框组件\nexport default {\n  components: {\n    \"contact-us\": ContactUs,\n    // 在这里注册你的组件\n    \"chat-box\": ChatBox // 注册你的聊天框组件\n  },\n\n  data() {\n    return {\n      words: [{\n        word: \"apple\",\n        definition: \"一种常见的水果\"\n      }, {\n        word: \"banana\",\n        definition: \"一种常见的黄色水果\"\n      }, {\n        word: \"cat\",\n        definition: \"一种家养的小型猫科动物\"\n      }, {\n        word: \"dog\",\n        definition: \"一种家养的哺乳动物\"\n      }, {\n        word: \"elephant\",\n        definition: \"一种大型陆生哺乳动物\"\n      }, {\n        word: \"fox\",\n        definition: \"一种小型犬科动物\"\n      }, {\n        word: \"grape\",\n        definition: \"一种常见的浆果\"\n      }, {\n        word: \"horse\",\n        definition: \"一种大型哺乳动物\"\n      }, {\n        word: \"iguana\",\n        definition: \"一种大型蜥蜴\"\n      }, {\n        word: \"jaguar\",\n        definition: \"一种大型猫科动物\"\n      }],\n      currentWordIndex: 0,\n      isDefinitionVisible: false,\n      searchTerm: \"\",\n      focusLostCountdown: 0,\n      countdownInterval: null,\n      // 新增一个变量来存储倒计时的interval\n      browsedWords: [] // 新增一个变量来存储已浏览的单词\n    };\n  },\n\n  methods: {\n    showDefinition() {\n      this.isDefinitionVisible = true;\n    },\n    created() {\n      const word = this.$route.params.word;\n      if (word) {\n        const index = this.words.findIndex(w => w.word === word);\n        if (index >= 0) {\n          this.currentWordIndex = index;\n          if (!this.browsedWords.includes(word)) {\n            this.browsedWords.push(word);\n          }\n        }\n      }\n    },\n    // ...\n\n    nextWord() {\n      const currentWord = this.words[this.currentWordIndex].word;\n      if (!this.browsedWords.includes(currentWord)) {\n        // 如果当前单词不在已浏览单词列表中，就添加到列表\n        this.browsedWords.push(currentWord);\n      }\n      this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n      this.isDefinitionVisible = false;\n      this.$router.push(`/words/${this.words[this.currentWordIndex].word}`);\n      if (!this.browsedWords.includes(this.words[this.currentWordIndex].word)) {\n        this.browsedWords.push(this.words[this.currentWordIndex].word);\n      }\n    },\n    // ...\n\n    jumpToWord(word) {\n      this.currentWordIndex = this.words.findIndex(w => w.word === word);\n      this.$router.push(`/words/${word}`);\n      if (!this.browsedWords.includes(word)) {\n        this.browsedWords.push(word);\n      }\n    },\n    searchWord() {\n      const index = this.words.findIndex(word => word.word === this.searchTerm);\n      if (index !== -1) {\n        this.currentWordIndex = index;\n        this.isDefinitionVisible = false;\n        this.searchMessage = \"\"; // 如果找到了单词，清空searchMessage\n        this.$router.push(`/words/${this.words[this.currentWordIndex].word}`); // 更新URL\n        if (!this.browsedWords.includes(this.words[this.currentWordIndex].word)) {\n          this.browsedWords.push(this.words[this.currentWordIndex].word); // 添加单词到browsedWords列表\n        }\n      } else {\n        this.searchMessage = \"未找到该单词\"; // 如果未找到单词，更新searchMessage\n      }\n    },\n\n    startCountdown() {\n      this.focusLostCountdown = 5;\n      this.countdownInterval = setInterval(() => {\n        if (this.focusLostCountdown > 0) {\n          this.focusLostCountdown--;\n        } else {\n          clearInterval(this.countdownInterval);\n        }\n      }, 1000);\n    },\n    stopCountdown() {\n      clearInterval(this.countdownInterval);\n      this.focusLostCountdown = 0;\n    },\n    clearBrowsedWords() {\n      this.browsedWords = [];\n    }\n  }\n};","map":{"version":3,"names":["ContactUs","ChatBox","components","data","words","word","definition","currentWordIndex","isDefinitionVisible","searchTerm","focusLostCountdown","countdownInterval","browsedWords","methods","showDefinition","created","$route","params","index","findIndex","w","includes","push","nextWord","currentWord","length","$router","jumpToWord","searchWord","searchMessage","startCountdown","setInterval","clearInterval","stopCountdown","clearBrowsedWords"],"sources":["H:\\单词学习项目\\word_study\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <!-- 添加一个头部 -->\r\n    <header>\r\n      <h1>Word Journeyman</h1>\r\n      <nav>\r\n        <a href=\"#\"></a>\r\n        <contact-us></contact-us>\r\n      </nav>\r\n    </header>\r\n\r\n    <!-- 添加侧边栏 -->\r\n    <div class=\"sidebar\">\r\n      <h2>单词探索历程</h2>\r\n      <ul>\r\n        <li v-for=\"word in browsedWords\" :key=\"word\" @click=\"jumpToWord(word)\">\r\n          {{ word }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <div class=\"center-container\">\r\n      <!-- 搜索栏 -->\r\n      <div class=\"search-bar\">\r\n        <input\r\n          v-model=\"searchTerm\"\r\n          type=\"text\"\r\n          placeholder=\"搜索单词...\"\r\n          @keyup.enter=\"searchWord\"\r\n        />\r\n        <button @click=\"searchWord\">搜索</button>\r\n        <p>{{ searchMessage }}</p>\r\n        <!-- 新增一行来显示searchMessage -->\r\n      </div>\r\n\r\n      <!-- 单词卡片 -->\r\n      <div\r\n        id=\"card\"\r\n        class=\"card\"\r\n        @mouseleave=\"startCountdown\"\r\n        @mouseenter=\"stopCountdown\"\r\n      >\r\n        <!-- 卡片内容 -->\r\n        <h1>{{ words[currentWordIndex].word }}</h1>\r\n\r\n        <!-- 定义显示 -->\r\n        <p v-show=\"isDefinitionVisible\">\r\n          {{ words[currentWordIndex].definition }}\r\n        </p>\r\n        <div class=\"card-buttons\">\r\n          <button\r\n            class=\"button\"\r\n            @click=\"showDefinition\"\r\n            :disabled=\"isDefinitionVisible\"\r\n          >\r\n            显示定义\r\n          </button>\r\n          <button class=\"button\" @click=\"nextWord\">下一个单词</button>\r\n        </div>\r\n\r\n        <!-- 小字提醒 -->\r\n        <p v-if=\"focusLostCountdown > 0\" class=\"countdown-message\">\r\n          焦点将在 {{ focusLostCountdown }} 秒后消失\r\n        </p>\r\n      </div>\r\n\r\n      <!-- 聊天框组件 -->\r\n      <chat-box></chat-box>\r\n    </div>\r\n\r\n    <!-- 添加一个底部 -->\r\n    <footer>\r\n      <p>©2023 Word Journeyman</p>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ContactUs from \"./components/ContactUs.vue\"; // 确保这里的路径是正确的\r\nimport ChatBox from \"./components/ChatBox.vue\"; // 引入你的聊天框组件\r\nexport default {\r\n  components: {\r\n    \"contact-us\": ContactUs, // 在这里注册你的组件\r\n    \"chat-box\": ChatBox, // 注册你的聊天框组件\r\n  },\r\n  data() {\r\n    return {\r\n      words: [\r\n        { word: \"apple\", definition: \"一种常见的水果\" },\r\n        { word: \"banana\", definition: \"一种常见的黄色水果\" },\r\n        { word: \"cat\", definition: \"一种家养的小型猫科动物\" },\r\n        { word: \"dog\", definition: \"一种家养的哺乳动物\" },\r\n        { word: \"elephant\", definition: \"一种大型陆生哺乳动物\" },\r\n        { word: \"fox\", definition: \"一种小型犬科动物\" },\r\n        { word: \"grape\", definition: \"一种常见的浆果\" },\r\n        { word: \"horse\", definition: \"一种大型哺乳动物\" },\r\n        { word: \"iguana\", definition: \"一种大型蜥蜴\" },\r\n        { word: \"jaguar\", definition: \"一种大型猫科动物\" },\r\n      ],\r\n      currentWordIndex: 0,\r\n      isDefinitionVisible: false,\r\n      searchTerm: \"\",\r\n      focusLostCountdown: 0,\r\n      countdownInterval: null, // 新增一个变量来存储倒计时的interval\r\n      browsedWords: [], // 新增一个变量来存储已浏览的单词\r\n    };\r\n  },\r\n  methods: {\r\n    showDefinition() {\r\n      this.isDefinitionVisible = true;\r\n    },\r\n    created() {\r\n      const word = this.$route.params.word;\r\n      if (word) {\r\n        const index = this.words.findIndex((w) => w.word === word);\r\n        if (index >= 0) {\r\n          this.currentWordIndex = index;\r\n          if (!this.browsedWords.includes(word)) {\r\n            this.browsedWords.push(word);\r\n          }\r\n        }\r\n      }\r\n    },\r\n\r\n    // ...\r\n\r\n    nextWord() {\r\n      const currentWord = this.words[this.currentWordIndex].word;\r\n      if (!this.browsedWords.includes(currentWord)) {\r\n        // 如果当前单词不在已浏览单词列表中，就添加到列表\r\n        this.browsedWords.push(currentWord);\r\n      }\r\n      this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\r\n      this.isDefinitionVisible = false;\r\n      this.$router.push(`/words/${this.words[this.currentWordIndex].word}`);\r\n      if (!this.browsedWords.includes(this.words[this.currentWordIndex].word)) {\r\n        this.browsedWords.push(this.words[this.currentWordIndex].word);\r\n      }\r\n    },\r\n\r\n    // ...\r\n\r\n    jumpToWord(word) {\r\n      this.currentWordIndex = this.words.findIndex((w) => w.word === word);\r\n      this.$router.push(`/words/${word}`);\r\n      if (!this.browsedWords.includes(word)) {\r\n        this.browsedWords.push(word);\r\n      }\r\n    },\r\n\r\n    searchWord() {\r\n      const index = this.words.findIndex(\r\n        (word) => word.word === this.searchTerm\r\n      );\r\n      if (index !== -1) {\r\n        this.currentWordIndex = index;\r\n        this.isDefinitionVisible = false;\r\n        this.searchMessage = \"\"; // 如果找到了单词，清空searchMessage\r\n        this.$router.push(`/words/${this.words[this.currentWordIndex].word}`); // 更新URL\r\n        if (\r\n          !this.browsedWords.includes(this.words[this.currentWordIndex].word)\r\n        ) {\r\n          this.browsedWords.push(this.words[this.currentWordIndex].word); // 添加单词到browsedWords列表\r\n        }\r\n      } else {\r\n        this.searchMessage = \"未找到该单词\"; // 如果未找到单词，更新searchMessage\r\n      }\r\n    },\r\n\r\n    startCountdown() {\r\n      this.focusLostCountdown = 5;\r\n      this.countdownInterval = setInterval(() => {\r\n        if (this.focusLostCountdown > 0) {\r\n          this.focusLostCountdown--;\r\n        } else {\r\n          clearInterval(this.countdownInterval);\r\n        }\r\n      }, 1000);\r\n    },\r\n    stopCountdown() {\r\n      clearInterval(this.countdownInterval);\r\n      this.focusLostCountdown = 0;\r\n    },\r\n\r\n    clearBrowsedWords() {\r\n      this.browsedWords = [];\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n@import url(\"../css/styles.css\");\r\n</style>"],"mappings":";AA+EA,OAAOA,SAAQ,MAAO,4BAA4B,EAAE;AACpD,OAAOC,OAAM,MAAO,0BAA0B,EAAE;AAChD,eAAe;EACbC,UAAU,EAAE;IACV,YAAY,EAAEF,SAAS;IAAE;IACzB,UAAU,EAAEC,OAAO,CAAE;EACvB,CAAC;;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAY,CAAC,EAC3C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAc,CAAC,EAC1C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAY,CAAC,EACxC;QAAED,IAAI,EAAE,UAAU;QAAEC,UAAU,EAAE;MAAa,CAAC,EAC9C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAW,CAAC,EACvC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAW,CAAC,EACzC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAS,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAW,CAAC,CAC3C;MACDC,gBAAgB,EAAE,CAAC;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,UAAU,EAAE,EAAE;MACdC,kBAAkB,EAAE,CAAC;MACrBC,iBAAiB,EAAE,IAAI;MAAE;MACzBC,YAAY,EAAE,EAAE,CAAE;IACpB,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACN,mBAAkB,GAAI,IAAI;IACjC,CAAC;IACDO,OAAOA,CAAA,EAAG;MACR,MAAMV,IAAG,GAAI,IAAI,CAACW,MAAM,CAACC,MAAM,CAACZ,IAAI;MACpC,IAAIA,IAAI,EAAE;QACR,MAAMa,KAAI,GAAI,IAAI,CAACd,KAAK,CAACe,SAAS,CAAEC,CAAC,IAAKA,CAAC,CAACf,IAAG,KAAMA,IAAI,CAAC;QAC1D,IAAIa,KAAI,IAAK,CAAC,EAAE;UACd,IAAI,CAACX,gBAAe,GAAIW,KAAK;UAC7B,IAAI,CAAC,IAAI,CAACN,YAAY,CAACS,QAAQ,CAAChB,IAAI,CAAC,EAAE;YACrC,IAAI,CAACO,YAAY,CAACU,IAAI,CAACjB,IAAI,CAAC;UAC9B;QACF;MACF;IACF,CAAC;IAED;;IAEAkB,QAAQA,CAAA,EAAG;MACT,MAAMC,WAAU,GAAI,IAAI,CAACpB,KAAK,CAAC,IAAI,CAACG,gBAAgB,CAAC,CAACF,IAAI;MAC1D,IAAI,CAAC,IAAI,CAACO,YAAY,CAACS,QAAQ,CAACG,WAAW,CAAC,EAAE;QAC5C;QACA,IAAI,CAACZ,YAAY,CAACU,IAAI,CAACE,WAAW,CAAC;MACrC;MACA,IAAI,CAACjB,gBAAe,GAAI
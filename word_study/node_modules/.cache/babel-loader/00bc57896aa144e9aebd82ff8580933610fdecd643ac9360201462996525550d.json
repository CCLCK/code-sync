n>\r\n      </form>\r\n      <form v-if=\"showVerifyForm\" @submit.prevent=\"verify\">\r\n        <h2>验证</h2>\r\n        <input v-model=\"verifyEmail\" type=\"email\" placeholder=\"电子邮件\">\r\n        <input v-model=\"verificationCode\" type=\"text\" placeholder=\"验证码\">\r\n        <button type=\"submit\">验证</button>\r\n      </form>\r\n      <button @click=\"closeForms\">关闭</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      registerEmail: '',\r\n      registerPassword: '',\r\n      loginEmail: '',\r\n      loginPassword: '',\r\n      verificationEmail: '',\r\n      verificationCode: '',\r\n      verifyEmail: '',\r\n      showRegisterForm: false,\r\n      showLoginForm: false,\r\n      showVerificationForm: false,\r\n      showVerifyForm: false,\r\n      showDropdownMenu: false,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleDropdownMenu() {\r\n      this.showDropdownMenu = !this.showDropdownMenu;\r\n    },\r\n    closeForms() {\r\n      this.showRegisterForm = false;\r\n      this.showLoginForm = false;\r\n      this.showVerificationForm = false;\r\n      this.showVerifyForm = false;\r\n    },\r\n    register() {\r\n      axios.post('http://localhost:5400/register', {\r\n        email: this.registerEmail,\r\n        password: this.registerPassword\r\n      })\r\n      .then(res => {\r\n        console.log(res.data.message);\r\n        this.closeForms();\r\n      })\r\n      .catch(err => {\r\n        console.error(err);\r\n      });\r\n    },\r\n    login() {\r\n      axios.post('http://localhost:5400/login', {\r\n        email: this.loginEmail,\r\n        password: this.loginPassword\r\n      })\r\n      .then(res => {\r\n        console.log(res.data.message);\r\n        this.closeForms();\r\n      })\r\n      .catch(err => {\r\n        console.error(err);\r\n      });\r\n    },\r\n    sendVerificationCode() {\r\n      axios.post('http://localhost:5400/send_verification_code', {\r\n        email: this.verificationEmail\r\n      })\r\n      .then(res => {\r\n        console.log(res.data.message);\r\n        this.closeForms();\r\n      })\r\n      .catch(err => {\r\n        console.error(err);\r\n      });\r\n    },\r\n    verify() {\r\n      axios.post('http://localhost:5400/verify', {\r\n        email: this.verifyEmail,\r\n        verification_code: this.verificationCode\r\n      })\r\n      .then(res => {\r\n        console.log(res.data.message);\r\n        this.closeForms();\r\n      })\r\n      .catch(err => {\r\n        console.error(err);\r\n      });\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal {\r\n  /* 根据需要定制你的模态窗口样式 */\r\n}\r\n\r\n.dropdown-menu {\r\n  /* 根据需要定制你的下拉菜单样式 */\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,gBAAgB,EAAE,EAAE;MACpBC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,iBAAiB,EAAE,EAAE;MACrBC,gBAAgB,EAAE,EAAE;MACpBC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,KAAK;MACvBC,aAAa,EAAE,KAAK;MACpBC,oBAAoB,EAAE,KAAK;MAC3BC,cAAc,EAAE,KAAK;MACrBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACF,gBAAe,GAAI,CAAC,IAAI,CAACA,gBAAgB;IAChD,CAAC;IACDG,UAAUA,CAAA,EAAG;MACX,IAAI,CAACP,gBAAe,GAAI,KAAK;MAC7B,IAAI,CAACC,aAAY,GAAI,KAAK;MAC1B,IAAI,CAACC,oBAAmB,GAAI,KAAK;MACjC,IAAI,CAACC,cAAa,GAAI,KAAK;IAC7B,CAAC;IACDK,QAAQA,CAAA,EAAG;MACTjB,KAAK,CAACkB,IAAI,CAAC,gCAAgC,EAAE;QAC3CC,KAAK,EAAE,IAAI,CAACjB,aAAa;QACzBkB,QAAQ,EAAE,IAAI,CAACjB;MACjB,CAAC,EACAkB,IAAI,CAACC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACrB,IAAI,CAACwB,OAAO,CAAC;QAC7B,IAAI,CAACT,UAAU,CAAC,CAAC;MACnB,CAAC,EACAU,KAAK,CAACC,GAAE,IAAK;QACZJ,OAAO,CAACK,KAAK,CAACD,GAAG,CAAC;MACpB,CAAC,CAAC;IACJ,CAAC;IACDE,KAAKA,CAAA,EAAG;MACN7B,KAAK,CAACkB,IAAI,CAAC,6BAA6B,EAAE;QACxCC,KAAK,EAAE,IAAI,CAACf,UAAU;QACtBgB,QAAQ,EAAE,IAAI,CAACf;MACjB,CAAC,EACAgB,IAAI,CAACC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACrB,IAAI,CAACwB,OAA
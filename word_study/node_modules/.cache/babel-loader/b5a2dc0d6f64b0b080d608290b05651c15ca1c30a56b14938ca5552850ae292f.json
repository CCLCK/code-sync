{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      showLogin: false,\n      showRegister: false,\n      email: '',\n      password: '',\n      user: null,\n      emailError: '',\n      rememberMe: false,\n      termsAccepted: false\n    };\n  },\n  methods: {\n    validateEmail() {\n      const re = /^[\\w-]+(\\.[\\w-]+)*@([\\w-]+\\.)+[a-zA-Z]{2,7}$/;\n      if (!re.test(this.email)) {\n        this.emailError = '请输入有效的电子邮件地址';\n      } else {\n        this.emailError = '';\n      }\n    },\n    login(event) {\n      event.preventDefault();\n      axios.post('http://localhost:5400/login', {\n        email: this.email,\n        password: this.password,\n        rememberMe: this.rememberMe\n      }).then(response => {\n        this.user = response.data;\n        this.showLogin = false;\n      }).catch(error => {\n        console.error('Error logging in:', error);\n      });\n    },\n    register(event) {\n      event.preventDefault();\n      axios.post('http://localhost:5400/register', {\n        email: this.email,\n        password: this.password,\n        verification_code: this.verificationCode\n      }).then(response => {\n        this.user = response.data;\n        this.showRegister = false;\n      }).catch(error => {\n        console.error('Error registering:', error);\n      });\n    },\n    logout() {\n      this.user = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","showLogin","showRegister","email","password","user","emailError","rememberMe","termsAccepted","methods","validateEmail","re","test","login","event","preventDefault","post","then","response","catch","error","console","register","verification_code","verificationCode","logout"],"sources":["H:\\单词学习项目\\word_study\\src\\components\\UserActions.vue"],"sourcesContent":["<template>\r\n  <el-row class=\"user-actions\">\r\n    <el-col v-if=\"!user\">\r\n      <el-button @click=\"showLogin = true\">登录</el-button>\r\n      <el-button @click=\"showRegister = true\">注册</el-button>\r\n    </el-col>\r\n    <el-col v-else>\r\n      <p>欢迎，{{ user.email }}</p>\r\n      <el-button @click=\"logout\">退出</el-button>\r\n    </el-col>\r\n\r\n    <el-dialog title=\"登录\" v-model=\"showLogin\">\r\n      <el-form @submit.prevent=\"login\">\r\n        <el-form-item label=\"邮箱\" :error=\"emailError\">\r\n          <el-input v-model=\"email\" placeholder=\"邮箱\" @input=\"validateEmail\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input v-model=\"password\" type=\"password\" placeholder=\"密码\" />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-checkbox v-model=\"rememberMe\">记住我</el-checkbox>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" native-type=\"submit\">登录</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"注册\" v-model=\"showRegister\">\r\n      <el-form @submit.prevent=\"register\">\r\n        <el-form-item label=\"邮箱\" :error=\"emailError\">\r\n          <el-input v-model=\"email\" placeholder=\"邮箱\" @input=\"validateEmail\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input v-model=\"password\" type=\"password\" placeholder=\"密码\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"验证码\">\r\n          <el-input v-model=\"verificationCode\" placeholder=\"验证码\" />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-checkbox v-model=\"termsAccepted\">我已阅读并接受服务条款</el-checkbox>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" native-type=\"submit\" :disabled=\"!termsAccepted\">注册</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n    \r\n  </el-row>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      showLogin: false,\r\n      showRegister: false,\r\n      email: '',\r\n      password: '',\r\n      user: null,\r\n      emailError: '',\r\n      rememberMe: false,\r\n      termsAccepted: false,\r\n    }\r\n  },\r\n  methods: {\r\n    validateEmail() {\r\n      const re = /^[\\w-]+(\\.[\\w-]+)*@([\\w-]+\\.)+[a-zA-Z]{2,7}$/\r\n      if (!re.test(this.email)) {\r\n        this.emailError = '请输入有效的电子邮件地址'\r\n      } else {\r\n        this.emailError = ''\r\n      }\r\n    },\r\n    login(event) {\r\n    event.preventDefault();\r\n\r\n    axios.post('http://localhost:5400/login', {\r\n      email: this.email,\r\n      password: this.password,\r\n      rememberMe: this.rememberMe\r\n    })\r\n    .then(response => {\r\n      this.user = response.data;\r\n      this.showLogin = false;\r\n      \r\n    })\r\n    .catch(error => {\r\n      console.error('Error logging in:', error);\r\n  \r\n    })\r\n  },\r\n  register(event) {\r\n      event.preventDefault();\r\n   \r\n      axios.post('http://localhost:5400/register', {\r\n        email: this.email,\r\n        password: this.password,\r\n        verification_code: this.verificationCode\r\n      })\r\n      .then(response => {\r\n        this.user = response.data;\r\n        this.showRegister = false;\r\n        \r\n      })\r\n      .catch(error => {\r\n        console.error('Error registering:', error);\r\n        \r\n      })\r\n    },\r\n    logout() {\r\n           this.user = null\r\n           \r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAqDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE,KAAK;MACnBC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,IAAI;MACVC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,KAAK;MACjBC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,MAAMC,EAAC,GAAI,8CAA6C;MACxD,IAAI,CAACA,EAAE,CAACC,IAAI,CAAC,IAAI,CAACT,KAAK,CAAC,EAAE;QACxB,IAAI,CAACG,UAAS,GAAI,cAAa;MACjC,OAAO;QACL,IAAI,CAACA,UAAS,GAAI,EAAC;MACrB;IACF,CAAC;IACDO,
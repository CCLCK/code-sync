{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ContactUs from \"./components/ContactUs.vue\"; // 确保这里的路径是正确的\nimport ChatBox from \"./components/ChatBox.vue\"; // 引入你的聊天框组件\nexport default {\n  components: {\n    \"contact-us\": ContactUs,\n    // 在这里注册你的组件\n    \"chat-box\": ChatBox // 注册你的聊天框组件\n  },\n\n  data() {\n    return {\n      words: [{\n        word: \"apple\",\n        definition: \"一种常见的水果\"\n      }, {\n        word: \"banana\",\n        definition: \"一种常见的黄色水果\"\n      }, {\n        word: \"cat\",\n        definition: \"一种家养的小型猫科动物\"\n      }, {\n        word: \"dog\",\n        definition: \"一种家养的哺乳动物\"\n      }, {\n        word: \"elephant\",\n        definition: \"一种大型陆生哺乳动物\"\n      }, {\n        word: \"fox\",\n        definition: \"一种小型犬科动物\"\n      }, {\n        word: \"grape\",\n        definition: \"一种常见的浆果\"\n      }, {\n        word: \"horse\",\n        definition: \"一种大型哺乳动物\"\n      }, {\n        word: \"iguana\",\n        definition: \"一种大型蜥蜴\"\n      }, {\n        word: \"jaguar\",\n        definition: \"一种大型猫科动物\"\n      }],\n      currentWordIndex: 0,\n      isDefinitionVisible: false,\n      searchTerm: \"\",\n      focusLostCountdown: 0,\n      countdownInterval: null,\n      // 新增一个变量来存储倒计时的interval\n      browsedWords: [] // 新增一个变量来存储已浏览的单词\n    };\n  },\n\n  methods: {\n    showDefinition() {\n      this.isDefinitionVisible = true;\n    },\n    nextWord() {\n      const currentWord = this.words[this.currentWordIndex].word;\n      if (!this.browsedWords.includes(currentWord)) {\n        // 如果当前单词不在已浏览单词列表中，就添加到列表\n        this.browsedWords.push(currentWord);\n      }\n      this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n      this.isDefinitionVisible = false;\n    },\n    searchWord() {\n      const index = this.words.findIndex(word => word.word === this.searchTerm);\n      if (index !== -1) {\n        this.currentWordIndex = index;\n        this.isDefinitionVisible = false;\n        this.searchMessage = \"\"; // 如果找到了单词，清空searchMessage\n      } else {\n        this.searchMessage = \"未找到该单词\"; // 如果未找到单词，更新searchMessage\n      }\n    },\n\n    startCountdown() {\n      this.focusLostCountdown = 5;\n      this.countdownInterval = setInterval(() => {\n        if (this.focusLostCountdown > 0) {\n          this.focusLostCountdown--;\n        } else {\n          clearInterval(this.countdownInterval);\n        }\n      }, 1000);\n    },\n    stopCountdown() {\n      clearInterval(this.countdownInterval);\n      this.focusLostCountdown = 0;\n    }\n  }\n};","map":{"version":3,"names":["ContactUs","ChatBox","components","data","words","word","definition","currentWordIndex","isDefinitionVisible","searchTerm","focusLostCountdown","countdownInterval","browsedWords","methods","showDefinition","nextWord","currentWord","includes","push","length","searchWord","index","findIndex","searchMessage","startCountdown","setInterval","clearInterval","stopCountdown"],"sources":["H:\\单词学习项目\\word_study\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- 添加一个头部 -->\n    <header>\n      <h1>Word Journeyman</h1>\n      <nav>\n        <a href=\"#\"></a>\n        <contact-us></contact-us>\n      </nav>\n    </header>\n\n    <!-- 添加侧边栏 -->\n    <div class=\"sidebar\">\n      <h2>已浏览单词</h2>\n      <ul>\n        <li v-for=\"word in browsedWords\" :key=\"word\" @click=\"jumpToWord(word)\">{{ word }}</li>\n      </ul>\n      <button @click=\"clearBrowsedWords\">清空已浏览单词</button>\n    </div>\n    \n\n    <div class=\"center-container\">\n      <!-- 搜索栏 -->\n      <div class=\"search-bar\">\n        <input\n          v-model=\"searchTerm\"\n          type=\"text\"\n          placeholder=\"搜索单词...\"\n          @keyup.enter=\"searchWord\"\n        />\n        <button @click=\"searchWord\">搜索</button>\n        <p>{{ searchMessage }}</p>  <!-- 新增一行来显示searchMessage -->\n      </div>\n\n      <!-- 单词卡片 -->\n      <div id=\"card\" class=\"card\" @mouseleave=\"startCountdown\" @mouseenter=\"stopCountdown\">\n        <!-- 卡片内容 -->\n        <h1>{{ words[currentWordIndex].word }}</h1>\n        <button @click=\"showDefinition\" :disabled=\"isDefinitionVisible\">显示定义</button>\n        <p v-show=\"isDefinitionVisible\">{{ words[currentWordIndex].definition }}</p>\n        <button @click=\"nextWord\">下一个单词</button>\n      \n        <!-- 小字提醒 -->\n        <p v-if=\"focusLostCountdown > 0\" class=\"countdown-message\">\n          焦点将在 {{ focusLostCountdown }} 秒后消失\n        </p>\n      </div>\n      \n       <!-- 聊天框组件 -->\n       <chat-box></chat-box>\n    </div>\n\n    <!-- 添加一个底部 -->\n    <footer>\n      <p>©2023 Word Journeyman</p>\n    </footer>\n  </div>\n</template>\n\n\n<script>\nimport ContactUs from \"./components/ContactUs.vue\"; // 确保这里的路径是正确的\nimport ChatBox from \"./components/ChatBox.vue\"; // 引入你的聊天框组件\nexport default {\n  components: {\n    \"contact-us\": ContactUs, // 在这里注册你的组件\n    \"chat-box\": ChatBox, // 注册你的聊天框组件\n  },\n  data() {\n    return {\n      words: [\n        { word: \"apple\", definition: \"一种常见的水果\" },\n        { word: \"banana\", definition: \"一种常见的黄色水果\" },\n        { word: \"cat\", definition: \"一种家养的小型猫科动物\" },\n        { word: \"dog\", definition: \"一种家养的哺乳动物\" },\n        { word: \"elephant\", definition: \"一种大型陆生哺乳动物\" },\n        { word: \"fox\", definition: \"一种小型犬科动物\" },\n        { word: \"grape\", definition: \"一种常见的浆果\" },\n        { word: \"horse\", definition: \"一种大型哺乳动物\" },\n        { word: \"iguana\", definition: \"一种大型蜥蜴\" },\n        { word: \"jaguar\", definition: \"一种大型猫科动物\" },\n      ],\n      currentWordIndex: 0,\n      isDefinitionVisible: false,\n      searchTerm: \"\",\n      focusLostCountdown: 0,\n      countdownInterval: null, // 新增一个变量来存储倒计时的interval\n      browsedWords: [], // 新增一个变量来存储已浏览的单词\n    };\n  },\n  methods: {\n    showDefinition() {\n      this.isDefinitionVisible = true;\n    },\n    nextWord() {\n  const currentWord = this.words[this.currentWordIndex].word;\n  if (!this.browsedWords.includes(currentWord)) {\n    // 如果当前单词不在已浏览单词列表中，就添加到列表\n    this.browsedWords.push(currentWord);\n  }\n  this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n  this.isDefinitionVisible = false;\n},\n\n    searchWord() {\n      const index = this.words.findIndex(\n        (word) => word.word === this.searchTerm\n      );\n      if (index !== -1) {\n        this.currentWordIndex = index;\n        this.isDefinitionVisible = false;\n        this.searchMessage = \"\";  // 如果找到了单词，清空searchMessage\n      } else {\n        this.searchMessage = \"未找到该单词\";  // 如果未找到单词，更新searchMessage\n      }\n    },\n    startCountdown() {\n      this.focusLostCountdown = 5;\n      this.countdownInterval = setInterval(() => {\n        if (this.focusLostCountdown > 0) {\n          this.focusLostCountdown--;\n        } else {\n          clearInterval(this.countdownInterval);\n        }\n      }, 1000);\n    },\n    stopCountdown() {\n      clearInterval(this.countdownInterval);\n      this.focusLostCountdown = 0;\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n@import url(\"../css/styles.css\");\n</style>"],"mappings":";AA6DA,OAAOA,SAAQ,MAAO,4BAA4B,EAAE;AACpD,OAAOC,OAAM,MAAO,0BAA0B,EAAE;AAChD,eAAe;EACbC,UAAU,EAAE;IACV,YAAY,EAAEF,SAAS;IAAE;IACzB,UAAU,EAAEC,OAAO,CAAE;EACvB,CAAC;;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAY,CAAC,EAC3C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAc,CAAC,EAC1C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAY,CAAC,EACxC;QAAED,IAAI,EAAE,UAAU;QAAEC,UAAU,EAAE;MAAa,CAAC,EAC9C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAW,CAAC,EACvC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAW,CAAC,EACzC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAS,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAW,CAAC,CAC3C;MACDC,gBAAgB,EAAE,CAAC;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,UAAU,EAAE,EAAE;MACdC,kBAAkB,EAAE,CAA
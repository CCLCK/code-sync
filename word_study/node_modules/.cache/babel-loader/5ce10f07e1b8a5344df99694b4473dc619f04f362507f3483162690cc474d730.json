{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ContactUs from './components/ContactUs.vue'; // 确保这里的路径是正确的\n\nexport default {\n  components: {\n    'contact-us': ContactUs // 在这里注册你的组件\n  },\n\n  data() {\n    return {\n      words: [{\n        word: \"apple\",\n        definition: \"一种常见的水果\"\n      }, {\n        word: \"banana\",\n        definition: \"一种常见的黄色水果\"\n      }, {\n        word: \"cat\",\n        definition: \"一种家养的小型猫科动物\"\n      }, {\n        word: \"dog\",\n        definition: \"一种家养的哺乳动物\"\n      }, {\n        word: \"elephant\",\n        definition: \"一种大型陆生哺乳动物\"\n      }, {\n        word: \"fox\",\n        definition: \"一种小型犬科动物\"\n      }, {\n        word: \"grape\",\n        definition: \"一种常见的浆果\"\n      }, {\n        word: \"horse\",\n        definition: \"一种大型哺乳动物\"\n      }, {\n        word: \"iguana\",\n        definition: \"一种大型蜥蜴\"\n      }, {\n        word: \"jaguar\",\n        definition: \"一种大型猫科动物\"\n      }],\n      currentWordIndex: 0,\n      isDefinitionVisible: false,\n      searchTerm: \"\",\n      focusLostCountdown: 0,\n      countdownInterval: null,\n      // 新增一个变量来存储倒计时的interval\n      browsedWords: [] // 新增一个变量来存储已浏览的单词\n    };\n  },\n\n  methods: {\n    showDefinition() {\n      this.isDefinitionVisible = true;\n    },\n    nextWord() {\n      this.browsedWords.push(this.words[this.currentWordIndex].word);\n      this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n      this.isDefinitionVisible = false;\n    },\n    searchWord() {\n      const index = this.words.findIndex(word => word.word === this.searchTerm);\n      if (index !== -1) {\n        this.currentWordIndex = index;\n        this.isDefinitionVisible = false;\n      } else {\n        alert(\"未找到该单词\");\n      }\n    },\n    startCountdown() {\n      this.focusLostCountdown = 5;\n      this.countdownInterval = setInterval(() => {\n        if (this.focusLostCountdown > 0) {\n          this.focusLostCountdown--;\n        } else {\n          clearInterval(this.countdownInterval);\n        }\n      }, 1000);\n    },\n    stopCountdown() {\n      clearInterval(this.countdownInterval);\n      this.focusLostCountdown = 0;\n    }\n  }\n};","map":{"version":3,"names":["ContactUs","components","data","words","word","definition","currentWordIndex","isDefinitionVisible","searchTerm","focusLostCountdown","countdownInterval","browsedWords","methods","showDefinition","nextWord","push","length","searchWord","index","findIndex","alert","startCountdown","setInterval","clearInterval","stopCountdown"],"sources":["H:\\单词学习项目\\word_study\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- 添加一个头部 -->\n    <header>\n      <h1>Word Journeyman</h1>\n      <div></div>  <!-- 新增一个空的div元素 -->\n      <nav>\n        \n        \n        <contact-us></contact-us>\n      </nav>\n    </header>\n    \n\n    <!-- 添加侧边栏 -->\n    <div class=\"sidebar\">\n      <h2>已浏览单词</h2>\n      <ul>\n        <li v-for=\"word in browsedWords\" :key=\"word\">{{ word }}</li>\n      </ul>\n    </div>\n\n    <!-- 搜索栏和卡片 -->\n    <div class=\"center-container\">\n      <!-- 搜索栏 -->\n      <div class=\"search-bar\">\n        <input\n          v-model=\"searchTerm\"\n          type=\"text\"\n          placeholder=\"搜索单词...\"\n          @keyup.enter=\"searchWord\"\n        />\n        <button @click=\"searchWord\">搜索</button>\n      </div>\n\n      <!-- 单词卡片 -->\n      <div id=\"card\" class=\"card\" @mouseleave=\"startCountdown\" @mouseenter=\"stopCountdown\">\n        <!-- 卡片内容 -->\n        <h1>{{ words[currentWordIndex].word }}</h1>\n        <button @click=\"showDefinition\" :disabled=\"isDefinitionVisible\">显示定义</button>\n        <p v-show=\"isDefinitionVisible\">{{ words[currentWordIndex].definition }}</p>\n        <button @click=\"nextWord\">下一个单词</button>\n      \n        <!-- 小字提醒 -->\n        <p v-if=\"focusLostCountdown > 0\" class=\"countdown-message\">\n          焦点将在 {{ focusLostCountdown }} 秒后消失\n        </p>\n      </div>\n      \n      <!-- <p v-if=\"focusLostCountdown > 0\" class=\"countdown countdown-message\">\n        焦点将在 {{ focusLostCountdown }} 秒后消失\n      </p> -->\n    </div>\n\n    \n\n    <!-- 添加一个底部 -->\n    <footer>\n      <p>©2023 Word Journeyman</p>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport ContactUs from './components/ContactUs.vue';  // 确保这里的路径是正确的\n\nexport default {\n  components: {\n    'contact-us': ContactUs,  // 在这里注册你的组件\n  },\n  data() {\n    return {\n      words: [\n        { word: \"apple\", definition: \"一种常见的水果\" },\n        { word: \"banana\", definition: \"一种常见的黄色水果\" },\n        { word: \"cat\", definition: \"一种家养的小型猫科动物\" },\n        { word: \"dog\", definition: \"一种家养的哺乳动物\" },\n        { word: \"elephant\", definition: \"一种大型陆生哺乳动物\" },\n        { word: \"fox\", definition: \"一种小型犬科动物\" },\n        { word: \"grape\", definition: \"一种常见的浆果\" },\n        { word: \"horse\", definition: \"一种大型哺乳动物\" },\n        { word: \"iguana\", definition: \"一种大型蜥蜴\" },\n        { word: \"jaguar\", definition: \"一种大型猫科动物\" },\n      ],\n      currentWordIndex: 0,\n      isDefinitionVisible: false,\n      searchTerm: \"\",\n      focusLostCountdown: 0,\n      countdownInterval: null, // 新增一个变量来存储倒计时的interval\n      browsedWords: [], // 新增一个变量来存储已浏览的单词\n    };\n  },\n  methods: {\n    showDefinition() {\n      this.isDefinitionVisible = true;\n    },\n    nextWord() {\n      this.browsedWords.push(this.words[this.currentWordIndex].word);\n      this.currentWordIndex = (this.currentWordIndex + 1) % this.words.length;\n      this.isDefinitionVisible = false;\n    },\n    searchWord() {\n      const index = this.words.findIndex(\n        (word) => word.word === this.searchTerm\n      );\n      if (index !== -1) {\n        this.currentWordIndex = index;\n        this.isDefinitionVisible = false;\n      } else {\n        alert(\"未找到该单词\");\n      }\n    },\n    startCountdown() {\n      this.focusLostCountdown = 5;\n      this.countdownInterval = setInterval(() => {\n        if (this.focusLostCountdown > 0) {\n          this.focusLostCountdown--;\n        } else {\n          clearInterval(this.countdownInterval);\n        }\n      }, 1000);\n    },\n    stopCountdown() {\n      clearInterval(this.countdownInterval);\n      this.focusLostCountdown = 0;\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n@import url(\"../css/styles.css\");\n</style>"],"mappings":";AAgEA,OAAOA,SAAQ,MAAO,4BAA4B,EAAG;;AAErD,eAAe;EACbC,UAAU,EAAE;IACV,YAAY,EAAED,SAAS,CAAG;EAC5B,CAAC;;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAY,CAAC,EAC3C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAc,CAAC,EAC1C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAY,CAAC,EACxC;QAAED,IAAI,EAAE,UAAU;QAAEC,UAAU,EAAE;MAAa,CAAC,EAC9C;QAAED,IAAI,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAW,CAAC,EACvC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAU,CAAC,EACxC;QAAED,IAAI,EAAE,OAAO;QAAEC,UAAU,EAAE;MAAW,CAAC,EACzC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAS,CAAC,EACxC;QAAED,IAAI,EAAE,QAAQ;QAAEC,UAAU,EAAE;MAAW,CAAC,CAC3C;MACDC,gBAAgB,EAAE,CAAC;MACnBC,mBAAmB,EAAE,KAAK;MAC1BC,UAAU,EAAE,EAAE;MACdC,kBAAkB,EAAE,CAAC;MACrBC,iBAAiB,EAAE,IAAI;MAAE;MACzBC,YAAY,EAAE,EAAE,CAAE;IACpB,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACN,mBAAkB,GAAI,IAAI;IACjC,CAAC;IACDO,QAAQA,CAAA,EAAG;MACT,IAAI,CAACH,YAAY,CAACI,IAAI,CAAC,IAAI,CAACZ,KAAK,CAAC,IAAI,CAACG,gBAAgB,CAAC,CAACF,IAAI,CAAC;MAC9D,IAAI,CAACE,gBAAe,GAAI,CAAC,IAAI,CAACA,gBAAe,GAAI,CAAC,IAAI,IAAI,CAACH,KAAK,CAACa,MAAM;MACvE,IAAI,CAACT,mBAAkB,GAAI,KAAK;IAClC,CAAC;IACDU,UAAUA,CAAA,EAAG;MACX,MAAMC,KAAI,GAAI,IAAI,CAACf,KAAK,CAACgB,SAAS,CAC/Bf,IAAI,IAAKA,IAAI,CAACA,IAAG,KAAM,IAAI,CAACI,UAC/B,CAAC;MACD,IAAIU,KAAI,KAAM,CAAC,CAAC,EAAE;QAChB,IAAI,CAACZ,gBAAe,GAAIY,KAAK;QAC7B,IAAI,CAACX,mBAAkB,GAAI,KAAK;MAClC,OAAO;QACLa,KAAK,CAAC,QAAQ,CAAC;MACjB;IACF,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACZ,kBAAiB,GAAI,CAAC;MAC3B,IAAI,CAACC,iBAAgB,GAA